{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": false
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "buildCommand": "npm run build:docs",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": "apps/docs/.next"
}
