{
  "name": "inam-ui",
  "version": "0.1.0",
  "description": "A powerful CLI for generating premium React components with TypeScript and Tailwind CSS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "inam-ui": "./dist/index.js"
  },
  "exports": {
    "./templates/*": "./src/templates/*.tsx"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:copy-templates",
    "build:ts": "tsc",
    "build:copy-templates": "node scripts/copy-templates.js",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "node tests/smoke-test.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "cli",
    "react",
    "components",
    "tailwindcss",
    "typescript",
    "ui-library",
    "component-library"
  ],
  "author": "Inam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inambymk/inam-ui"
  },
  "homepage": "https://github.com/inambymk/inam-ui#readme",
  "bugs": {
    "url": "https://github.com/inambymk/inam-ui/issues"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15",
    "mute-stream": "0.0.8",
    "posthog-node": "^5.18.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/mute-stream": "^0.0.4",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
